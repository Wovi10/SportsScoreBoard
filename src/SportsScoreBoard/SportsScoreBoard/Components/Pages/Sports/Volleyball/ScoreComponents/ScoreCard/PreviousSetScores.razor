@using SportsScoreBoard.Models.Volleyball
<MudStack Row="true" Class="mx-5 justify-center" Wrap="Wrap.Wrap" AlignItems="AlignItems.Center">
    @foreach(var setScore in SetScores)
    {
        <MudPaper Class="px-3 py-2">
            <MudText Typo="Typo.h3" Align="Align.Center">@setScore</MudText>
        </MudPaper>
    }
</MudStack>

@code{
    [Parameter] public required List<SetScore> SetScores { get; set; }
}