@rendermode InteractiveServer

<MudGrid>
    <MudItem xs="12">
        <MudText Typo="Typo.h3" Align="Align.Center">Volleyball</MudText>
    </MudItem>
    <TeamScoreCard
        Team="@Game.Home"
        Score="@Game.Score.Home"
        IncrementScore="IncrementHome" 
        ChangeTeamName="HomeNameChanged" />
    <TeamScoreCard
        Team="@Game.Away"
        Score="@Game.Score.Away"
        IncrementScore="IncrementAway"
        ChangeTeamName="AwayNameChanged" />
</MudGrid>